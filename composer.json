{
  "name": "koma136/mytarget-sdk-php",
  "description": "API bindings for MyTarget",
  "type": "library",
  "require": {
    "yiisoft/yii2": "~2.0.6",
    "doctrine/instantiator": "^1",
    "psr/http-message": "~1",
    "guzzlehttp/psr7": "^1",
    "doctrine/annotations": "^1",
    "doctrine/cache": "^1"
  },
  "require-dev": {
    "phpunit/phpunit": "^4.7",
    "guzzlehttp/guzzle": "^6",
    "dsl/cache-control": "0.1.0",
    "psr/log": "^1"
  },
  "license": "Apache-2.0",
  "autoload": {
    "psr-4": {"Koma136\\MyTarget\\": "src/", "tests\\Koma136\\MyTarget\\": "tests/"},
    "files": ["src/functions.php"]
  },
  "config": {
    "process-timeout": 1800,
    "fxp-asset": {
      "enabled": false
    }
  },
  "repositories": [
    {
      "type": "composer",
      "url": "https://asset-packagist.org"
    }
  ]
}
